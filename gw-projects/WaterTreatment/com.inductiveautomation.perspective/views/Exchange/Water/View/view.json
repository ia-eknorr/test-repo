{
  "custom": {},
  "params": {
    "filterNum": 1,
    "screen": "dashboard"
  },
  "propConfig": {
    "params.filterNum": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.screen": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "name": "Main"
        },
        "position": {
          "basis": "320px",
          "grow": 1
        },
        "propConfig": {
          "props.params": {
            "binding": {
              "config": {
                "path": "view.params.filterNum"
              },
              "transforms": [
                {
                  "code": "\tif value !\u003d None:\n\t\treturn {\u0027filterNum\u0027: value}",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "expression": "{view.params.screen}"
              },
              "transforms": [
                {
                  "code": "\tconvertedPath \u003d value.replace(\u0027_\u0027, \u0027 \u0027).title()\n\tif not convertedPath:\n\t\tconvertedPath \u003d \u0027Dashboard\u0027\n\tif convertedPath \u003d\u003d \"Chemfeed\":\n\t\tconvertedPath \u003d \"ChemFeed\"\n\treturn \"Exchange/Water/Views/\" + convertedPath if convertedPath else \u0027Dashboard\u0027 ",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}